{
  "parser": "@typescript-eslint/parser",

  "env": {
    "node": true,
    "es6": true
  },

  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },

  "extends": [
    "airbnb-base",
    "prettier"
  ],

  "plugins": ["prettier"],

  "rules": {
    "prettier/prettier": "warn",
    "import/extensions": "off",
    "import/no-unresolved": "off",
    "import/prefer-default-export": "off"
  }
}
